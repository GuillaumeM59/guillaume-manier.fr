<div class="container">

  <header class="row">
    <div class="identity col-sm-12 col-md-5 col-lg-5">
      <div class="row">

        <div class="idtexte col-sm-7">
          <h1>Guillaume Manier</h1>
          <h5 class="text-center">Mobile & Web Developper</h6>

          <div class="progress">
            <div class="progress-bar progress-bar-info progress-bar-striped active" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width: 70%">
            <span class="sr-only">70% Complete</span>
            </div>
          </div>

          <p class="grade text-center">(junior)</p>
        </div>

        <div class="idpic col-sm-3">
          <img src="/img/moi.jpg" alt="Guillaume Manier" class="img-circle img-responsive"/>
        </div>

      </div>
    </div>

    <div class="followme col-sm-4 col-md-3 col-lg-3">
      <h3>Follow me</h3>
      <p class="iconsfollow text-center">
        <i class="fa fa-facebook-square fa-3x"></i>
        <i class="fa fa-twitter-square fa-3x"></i>
        <i class="fa fa-github-square fa-3x"></i>
        <i class="fa fa-linkedin-square fa-3x"></i>
      </p>
    </div>
    <div class="getinfos col-sm-12 col-md-4 col-lg-4">
      <div class="row">

        <div class="subscribe col-sm-6">
          <h3>Subscribe</h3>
          <p class="text-center">
            <button type="button" name="button"><i class="fa fa-envelope-o fa-3x"></i> Newsletter</button>
          </p>
        </div>

        <div class="getcv col-sm-6">
          <h3>Get my CV</h3>
          <p class="cvbtns text-center">
            <button type="button" name="button"class="cvseebtn">
              <i class="fa fa-eye fa-3x"></i>
            </button>
            <button type="button" name="button" class="cvdlbtn">
              <i class="fa fa-download fa-3x"></i>
            </button>
          </p>
        </div>

      </div>
    </div>
  </header>


  <div class="row">
    <section class="col-sm-12 col-md-8 col-lg-9">
      <div class="row">

        <div class="aboutme panel col-sm-12 col-md-8 col-lg-7">
          <h4>About Me</h4>
          <p>

          </p>
          <button type="button" name="button"><i class="fa fa-plus"></i> more meetings</button>
        </div>
        <div class="meeting panel col-sm-12 col-md-4 col-lg-4 col-lg-offset-1">
          <h4>Next Meetings</h4>

          <button type="button" name="button"><i class="fa fa-plus"></i> more meetings</button>
        </div>
        <div class="carou panel col-sm-12 col-md-12 col-lg-12">
          <h4>Latest Projects</h4>

          <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
              <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
              <li data-target="#carousel-example-generic" data-slide-to="1"></li>
              <li data-target="#carousel-example-generic" data-slide-to="2"></li>
            </ol>

            <!-- Wrapper for slides -->
            <div class="carousel-inner" role="listbox">
              <div class="item active">
                <img src="..." alt="...">
                <div class="carousel-caption">
                  ...
                </div>
              </div>
              <div class="item">
                <img src="..." alt="...">
                <div class="carousel-caption">
                  ...
                </div>
              </div>
              ...
            </div>

            <!-- Controls -->
            <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
              <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
              <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
          </div>

          <button type="button" name="button"><i class="fa fa-plus"></i> more meetings</button>
        </div>
      </div>
    </section>
    <article class="col-sm-12 col-md-4 col-lg-3">
      <div class="madewith panel col-sm-12">
        <h4>Made with</h4>

      </div>
      <div class="madewith panel col-sm-12">
        <h4>Learning Progress</h4>
        <ul class="learn list-unstyled">
          <% Language.all.each do |lang| %>
          <li class="row">
            <div class="col-sm-9">
              <div class="progress ">
                <div class="progress-bar progress-bar-info progress-bar-striped active lang-bar" role="progressbar" aria-valuenow="<%=lang.progress%>" aria-valuemin="0" aria-valuemax="100" style="width: <%=lang.progress%>%">
                  <%=lang.name%>
                  <span class="sr-only"><%=lang.progress%>% Complete</span>
                </div>
              </div>
            </div>
          </li>
          <%end%>
        </ul>
        <button type="button" name="button"><i class="fa fa-plus"></i> see all techno.</button>
      </div>
    </article>






































</div>
